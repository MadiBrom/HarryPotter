datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  // Ensure your .env file contains the DATABASE_URL variable
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id       String     @id @default(uuid())
  username String
  email    String  @unique
  password String
  testResults TestResult[] // Relationship with test results
}

model TestResult {
  id          String   @id @default(uuid())
  userId      String
  houseResult String
  user        User     @relation(fields: [userId], references: [id])
}

